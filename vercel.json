{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "." }
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "api/bot.py"
    },
    {
      "src": "/setwebhook",
      "dest": "api/bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
