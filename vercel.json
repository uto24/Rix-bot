{
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "api/bot.py"
    },
    {
      "src": "/setwebhook",
      "dest": "api/bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
